{
    "logging": {
        "name": "test-opentelemetry",
        "level": "DEBUG",
        "formatter": "%(asctime)s - %(levelname)s - %(message)s",
        "http_client": {
            "enabled": false
        },
        "console": {
            "enabled": true
        },
        "file": {
            "enabled": true,
            "max_size_mb": "50",
            "max_file_count": "1",
            "path": "log.log"
        },
        "otlp": {
            "enabled": false,
            "endpoint": "http://otel-collector:4317",
            "insecure": true,
            "use_credentials": false,
            "username": "",
            "password": "",
            "max_attempts": 1
        }
    },
    "queue": {
        "username": "guest",
        "password": "guest",
        "virtual_host": "/",
        "port": 5672,
        "host": "rabbitmq",
        "assembly_name": "test-opentelemetry",
        "exchange_name": "test-opentelemetry",
        "heartbeat": 900
    },
    "trace": {
        "enabled": true,
        "service_name": "test-opentelemetry",
        "otlp": {
            "endpoint": "http://otel-collector:4317",
            "insecure": true,
            "use_credentials": false,
            "use_ssl_certificate": false,
            "ssl_certificate_path": "./core/trace/certificate",
            "token": ""
        }
    },
    "metrics": {
        "enabled": true,
        "endpoint": "http://otel-collector:4317",
        "insecure": true,
        "use_credentials": false,
        "use_ssl_certificate": false,
        "ssl_certificate_path": "./core/trace/certificate",
        "token": "",
        "service_name": "test-opentelemetry.opentelemetry",
        "library_name": "test-opentelemetry.extractor",
        "library_version": "0.1.0"
    },
    "redis": {
        "host": "redis",
        "port": 6379,
        "db": 0
    }
}
